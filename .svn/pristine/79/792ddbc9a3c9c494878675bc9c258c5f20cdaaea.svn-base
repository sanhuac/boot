 * @Date: 2018/9/10 15:37
 * @Description:
 */
@Data
public class StoreImageModel implements Serializable {

    /**
     * 商店id
     */
    private Integer mer_id;

    /**
     * 图片地址
     */
    @ApiModelProperty(value = "图片地址",name = "image_url",example = "images/school/1534236090914.jpg")
    private String image_url;

    /**
     * 图片类型[0:店铺首页图]
     */
    @ApiModelProperty(value = "图片类型[0:店铺首页图]",name = "image_type",example = "0")
    private Integer image_type;

    /**
     * 开始时间
     */
    @ApiModelProperty(value = "开始时间",name = "start_time",example = "1536551257")
    private Integer start_time;

    /**
     * 结束时间
     */
    @ApiModelProperty(value = "结束时间",name = "end_time",example = "1536551257")
