import io.swagger.annotations.ApiModelProperty;

import java.io.Serializable;

/**
 * 登录值对象
 *
 * @author pht
 * @program merchants_cms
 * @date 2018/9/7 0007
 */
public class LoginModel implements Serializable {

    private static final long serialVersionUID = 1062974703893097472L;

    //商户id
    private Integer mer_id;
    //商户手机号
    private String mer_mobile;
    //密码
    private String password;
    //商户法人名称
    private String mer_name;
    //商户状态
    private Integer mer_status;

    public Integer getMer_id() {
        return mer_id;
    }

    public void setMer_id(Integer mer_id) {
        this.mer_id = mer_id;
    }

    public String getMer_mobile() {
        return mer_mobile;
    }

    public void setMer_mobile(String mer_mobile) {
        this.mer_mobile = mer_mobile;
    }

    public String getPassword() {
        return password;
    }

    public void setPassword(String password) {
        this.password = password;
    }

    public String getMer_name() {
        return mer_name;
    }

    public void setMer_name(String mer_name) {
        this.mer_name = mer_name;
    }

    public Integer getMer_status() {
        return mer_status;
    }

    public void setMer_status(Integer mer_status) {
        this.mer_status = mer_status;
    }

    @Override
    public String toString() {
        return "LoginModel{" +
                "mer_id=" + mer_id +
                ", mer_mobile='" + mer_mobile + '\'' +
                ", password='" + password + '\'' +
                ", mer_name='" + mer_name + '\'' +
                ", mer_status=" + mer_status +
                '}';
    }
}
